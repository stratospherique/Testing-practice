{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "allowImportExportEverywhere": false,
        "codeFrame": true
    },
    "rules": {
        "strict": 0,
        "no-undef": 0
    }
}